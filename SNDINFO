$random AA/Computer/Beep	{ DSBOOP1 DSBOOP2 DSBOOP3 DSBOOP4 DSBOOP5
							DSBOOP6 DSBOOP7 DSBOOP8 DSBOOP9 DSBOOP10 DSBOOP11
							DSBOOP12 DSBOOP13 DSBOOP14 DSBOOP15 DSBOOP16
							DSBOOP17 DSBOOP18 DSBOOP19 DSBOOP20 }

$random AA/Computer/Boop	{ DSBEEP1 DSBEEP2 DSBEEP3 DSBEEP4 DSBEEP5
							DSBEEP6 DSBEEP7 DSBEEP8 DSBEEP9 DSBEEP10 DSBEEP11
							DSBEEP12 DSBEEP13 DSBEEP14 DSBEEP15 DSBEEP16
							 DSBEEP17 }

$attenuation AA/Computer/Beep 5
$volume AA/Computer/Beep 0.4
$attenuation AA/Computer/Boop 5
$volume AA/Computer/Boop 0.4

AA/Pipes/v1	DSASPIPS
AA/Pipes/v2	DSASWAT1
AA/Pipes/v3	DSASGOOP

$random AA/Pipes { AA/Pipes/v1 AA/Pipes/v2 AA/Pipes/v3 }
$attenuation AA/Pipes 4
$volume AA/Pipes 0.4

